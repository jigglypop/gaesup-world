{
  "name": "gaesup-world",
  "version": "0.9.67",
  "type": "module",
  "description": "Web 3D Character Controller and World Platform Library",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "dev": "vite --force",
    "clean": "rimraf dist/",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs && pnpm build:types",
    "build:esm": "vite build --mode esm",
    "build:cjs": "vite build --mode cjs",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prepare": "pnpm build",
    "predeploy": "pnpm build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:memory": "jest --testPathPattern=memory --verbose",
    "test:memory:verbose": "node scripts/run-memory-tests.js --verbose",
    "test:memory:ci": "node scripts/run-memory-tests.js --ci",
    "release": "semantic-release",
    "publish:full": "pnpm prepare && npm publish"
  },
