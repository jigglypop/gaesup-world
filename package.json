{
  "name": "gaesup-world",
  "version": "0.9.68",
  "type": "commonjs",
  "main": "./index.ts",
  "types": "./index.d.ts",
  "typesVersions": {
    ">=4.5": {
      "esm/*": [
        "esm/*"
      ],
      "*": [
        "*"
      ]
    },
    "*": {
      "esm/*": [
        "ts_version_4.5_and_above_is_required.d.ts"
      ],
      "*": [
        "ts_version_4.5_and_above_is_required.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./esm/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./esm/*.d.mts",
        "default": "./esm/*.mjs"
      },
      "default": {
        "types": "./*.d.ts",
        "default": "./*.js"
      }
    }
  },
  "files": [
    "**"
  ],
  "homepage": "https://jigglypop.github.io/gaesup-world/",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.ts src/**/*.tsx dist/",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "yarn clean && tsc -p tsconfig.json && yarn copy-files",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "publish:full": "yarn prepare && npm publish"
  },
  "peerDependencies": {
    "@dimforge/rapier3d": "^0.14.0",
    "@dimforge/rapier3d-compat": "^0.14.0",
    "@react-three/drei": "9.112.0",
    "@react-three/fiber": "^8.17.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "three": "^0.168.0"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@react-three/postprocessing": "^2.16.2",
    "@react-three/rapier": "^1.4.0",
    "@types/lodash": "^4.17.9",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.168.0",
    "gh-pages": "^6.1.1",
    "jotai": "^2.10.0",
    "leva": "0.9.35",
    "lodash": "^4.17.21",
    "lru-cache": "^11.0.1",
    "node-fetch": "3.3.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-use-refs": "^1.0.1",
    "three": "^0.168.0",
    "three-stdlib": "^2.33.0"
  },
  "devDependencies": {
    "@vanilla-extract/css": "^1.15.5",
    "@vanilla-extract/dynamic": "^2.1.2",
    "@vanilla-extract/recipes": "^0.5.5",
    "@vanilla-extract/sprinkles": "^1.6.3",
    "@vanilla-extract/vite-plugin": "^4.0.15",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "husky": "^9.1.6",
    "path": "^0.12.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-plugin-svgr": "^4.2.0"
  },
  "description": "Web 3D Character Controller and World Platform Library",
  "repository": {
    "type": "git",
    "url": "https://github.com/jigglypop/gaesup-world.git"
  },
  "keywords": [
    "three.js",
    "@react-three/fiber",
    "@react-three/drei",
    "leva",
    "@react-three/rapier",
    "rapier"
  ],
  "author": "jigglypop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jigglypop/gaesup-world.git/issues"
  },
  "packageManager": "yarn@4.4.1"
}
