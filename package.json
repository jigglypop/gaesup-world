{
  "name": "gaesup-world",
  "version": "0.9.67",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "homepage": "https://jigglypop.github.io/gaesup-world/",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.ts src/**/*.tsx dist/",
    "build": "vite build",
    "build:lib": "tsc -p tsconfig.build.json && yarn copy-files",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "preview": "vite preview",
    "prepare": "yarn clean && yarn build:lib",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "test": "jest --config=jest.config.cjs",
    "test:watch": "jest --config=jest.config.cjs --watch",
    "test:coverage": "jest --config=jest.config.cjs --coverage",
    "publish:full": "yarn prepare && npm publish"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react-use-refs": "^1.0.1",
    "three": "^0.168.0",
    "three-stdlib": "^2.32.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@dimforge/rapier3d": "^0.14.0",
    "@dimforge/rapier3d-compat": "^0.14.0",
    "@react-three/drei": "9.112.0",
    "@react-three/fiber": "^8.17.7",
    "@react-three/postprocessing": "^2.16.2",
    "@react-three/rapier": "^1.4.0",
    "@react-three/test-renderer": "^9.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.168.0",
    "@vanilla-extract/css": "^1.15.5",
    "@vanilla-extract/dynamic": "^2.1.2",
    "@vanilla-extract/recipes": "^0.5.5",
    "@vanilla-extract/sprinkles": "^1.6.3",
    "@vanilla-extract/vite-plugin": "^4.0.15",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "babel-jest": "^29.7.0",
    "copyfiles": "^2.4.1",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jotai": "^2.9.3",
    "prettier": "^3.3.3",
    "react": "^18.0.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.0.0",
    "react-icons": "^5.3.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vite-plugin-svgr": "^4.2.0"
  },
  "peerDependencies": {
    "@dimforge/rapier3d": "^0.14.0",
    "@dimforge/rapier3d-compat": "^0.14.0",
    "@react-three/drei": "^9.0.0",
    "@react-three/fiber": "^8.0.0",
    "@react-three/rapier": "^1.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "three": "^0.150.0 || ^0.168.0"
  },
  "description": "Web 3D Character Controller and World Platform Library",
  "repository": {
    "type": "git",
    "url": "https://github.com/jigglypop/gaesup-world.git"
  },
  "keywords": [
    "three.js",
    "@react-three/fiber",
    "@react-three/drei",
    "@react-three/rapier",
    "rapier",
    "3d",
    "character-controller",
    "game"
  ],
  "author": "jigglypop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jigglypop/gaesup-world/issues"
  },
  "packageManager": "yarn@4.4.1",
  "engines": {
    "node": ">=16"
  }
}
