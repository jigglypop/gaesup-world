{
  "name": "gaesup-world",
  "version": "0.9.67",
  "type": "module",
  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "homepage": "https://jigglypop.github.io/gaesup-world/",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.ts src/**/*.tsx dist/",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "yarn clean && tsc -p tsconfig.build.json && yarn copy-files",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:memory": "jest --testPathPattern=memory --verbose",
    "test:memory:verbose": "node scripts/run-memory-tests.js --verbose",
    "test:memory:ci": "node scripts/run-memory-tests.js --ci",
    "publish:full": "yarn prepare && npm publish"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@react-three/postprocessing": "^2.16.2",
    "@types/lodash": "^4.17.7",
    "@types/three": "^0.168.0",
    "gh-pages": "^6.1.1",
    "jotai": "^2.9.3",
    "leva": "0.9.35",
    "lodash": "^4.17.21",
    "lru-cache": "^11.0.1",
    "node-fetch": "3.3.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-use-refs": "^1.0.1",
    "three": "^0.168.0",
    "three-stdlib": "^2.32.2"
  },
  "devDependencies": {
    "@dimforge/rapier3d": "^0.14.0",
    "@dimforge/rapier3d-compat": "^0.14.0",
    "@react-three/drei": "9.112.0",
    "@react-three/fiber": "^8.17.7",
    "@react-three/rapier": "^1.4.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vanilla-extract/css": "^1.15.5",
    "@vanilla-extract/dynamic": "^2.1.2",
    "@vanilla-extract/recipes": "^0.5.5",
    "@vanilla-extract/sprinkles": "^1.6.3",
    "@vanilla-extract/vite-plugin": "^4.0.15",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "import-local": "^3.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "path": "^0.12.7",
    "prettier": "^3.3.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "turbo": "^2.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vite-plugin-svgr": "^4.2.0"
  },
  "peerDependencies": {
    "@dimforge/rapier3d": "^0.14.0",
    "@dimforge/rapier3d-compat": "^0.14.0",
    "@react-three/drei": "9.112.0",
